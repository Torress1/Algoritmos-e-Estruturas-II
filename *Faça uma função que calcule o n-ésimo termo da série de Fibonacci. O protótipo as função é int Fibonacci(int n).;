#include <stdio.h>


int fibo (int n) {
  int a = 1, b = 1, fib=1;
  for  (int i=3; i <= n; i++){
      fib = a + b;
      a = b;
      b = fib;
  }
  return fib;
}


int main () {
    int a, b, i, n, fib;
    
    printf("Digite um valor maior que zero: ");
    scanf("%d", &n);
    
    
    printf ("O valor resultado Ã©: = %d", fibo(n));
    return 0;
}
    

